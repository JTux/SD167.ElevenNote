@model ElevenNote.Models.User.UserDetail
@{
    ViewData["Title"] = "Profile";
}

<h1 class="display-4">
    Your Profile
</h1>

<div>
    <div>Username: @Model.Username</div>
    <div>Email: @Model.Email</div>
    <div>First Name: @(Model.FirstName ?? "N/A")</div>
    <div>Last Name: @(Model.LastName ?? "N/A")</div>
    <div>
        Member since: @Model.DateCreated
    </div>
</div>